<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context=".CodeSetActivity">

    <com.dawn.newlandscan.widget.TopToolbar
        android:id="@+id/tb_topToolBar"
        android:layout_width="match_parent"
        android:layout_height="wrap_content">
    </com.dawn.newlandscan.widget.TopToolbar>

    <ListView
        android:id="@+id/gv_codeSetMenu"
        android:layout_marginTop="@dimen/device_setting_gridView_margin_top"
        android:layout_below="@+id/tb_topToolBar"
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        android:numColumns="auto_fit"
        android:stretchMode="columnWidth"
        android:verticalSpacing="0dp">
    </ListView>

</RelativeLayout>